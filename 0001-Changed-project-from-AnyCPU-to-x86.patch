+            HorizontalAlignment="Stretch" Margin="5,0,5,0" VerticalAlignment="Center" Color="White" />
+          <ListView x:Name="lvCountrys" Grid.Column="0" Grid.Row="2" Margin="10" 
+            HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
